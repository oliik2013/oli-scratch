{
  "name": "scratch-lego-bluetooth-extensions",
  "version": "0.1.0",
  "description": "Scratch 3.0 extensions for LEGO Bluetooth and BLE devices",
  "scripts": {
    "register": "npm run register:legoble && npm run register:spikeessential && npm run register:legoremote && npm run register:controlplus && npm run register:poweredup && npm run register:duplotrain && npm run register:legopeach && npm run register:legoluigi && npm run register:legomario && npm run register:qrcode",
    "register:spikeprime": "node ./scripts/register.js --link -C --id=\"spikeprime\" --block=\"./scratch-vm/src/extensions/scratch3_spikeprime\" --entry=\"./scratch-gui/src/lib/libraries/extensions/spikeprime\" --gui=\"../scratch-gui\"",
    "register:legoble": "node ./scripts/register.js --link -C --id=\"legoble\" --block=\"./scratch-vm/src/extensions/scratch3_legoble\" --entry=\"./scratch-gui/src/lib/libraries/extensions/legoble\" --gui=\"../scratch-gui\"",
    "register:spikeessential": "node ./scripts/register.js --link -C --id=\"spikeessential\" --block=\"./scratch-vm/src/extensions/scratch3_spikeessential\" --entry=\"./scratch-gui/src/lib/libraries/extensions/spikeessential\" --gui=\"../scratch-gui\"",
    "register:legoremote": "node ./scripts/register.js --link -C --id=\"legoremote\" --block=\"./scratch-vm/src/extensions/scratch3_legoremote\" --entry=\"./scratch-gui/src/lib/libraries/extensions/legoremote\" --gui=\"../scratch-gui\"",
    "register:controlplus": "node ./scripts/register.js --link -C --id=\"controlplus\" --block=\"./scratch-vm/src/extensions/scratch3_controlplus\" --entry=\"./scratch-gui/src/lib/libraries/extensions/controlplus\" --gui=\"../scratch-gui\"",
    "register:poweredup": "node ./scripts/register.js --link -C --id=\"poweredup\" --block=\"./scratch-vm/src/extensions/scratch3_poweredup\" --entry=\"./scratch-gui/src/lib/libraries/extensions/poweredup\" --gui=\"../scratch-gui\"",
    "register:duplotrain": "node ./scripts/register.js --link -C --id=\"duplotrain\" --block=\"./scratch-vm/src/extensions/scratch3_duplotrain\" --entry=\"./scratch-gui/src/lib/libraries/extensions/duplotrain\" --gui=\"../scratch-gui\"",
    "register:legomario": "node ./scripts/register.js --link -C --id=\"legomario\" --block=\"./scratch-vm/src/extensions/scratch3_legomario\" --entry=\"./scratch-gui/src/lib/libraries/extensions/legomario\" --gui=\"../scratch-gui\"",
    "register:legoluigi": "node ./scripts/register.js --link -C --id=\"legoluigi\" --block=\"./scratch-vm/src/extensions/scratch3_legoluigi\" --entry=\"./scratch-gui/src/lib/libraries/extensions/legoluigi\" --gui=\"../scratch-gui\"",
    "register:legopeach": "node ./scripts/register.js --link -C --id=\"legopeach\" --block=\"./scratch-vm/src/extensions/scratch3_legopeach\" --entry=\"./scratch-gui/src/lib/libraries/extensions/legopeach\" --gui=\"../scratch-gui\"",
    "register:qrcode": "node ./scripts/register.js --link -C --id=\"qrcode\" --block=\"./scratch-vm/src/extensions/scratch3_qrcode\" --entry=\"./scratch-gui/src/lib/libraries/extensions/qrcode\" --gui=\"../scratch-gui\"",
    "build": "npm run build:legoble && npm run build:spikeessential && npm run build:legoremote && npm run build:controlplus && npm run build:poweredup && npm run build:duplotrain && npm run build:legopeach && npm run build:legoluigi && npm run build:legomario",
    "build:spikeprime": "npx xcratch-build --module=\"spikeprime\" --block=\"scratch-vm/src/extensions/scratch3_spikeprime\" --entry=\"scratch-gui/src/lib/libraries/extensions/spikeprime\" --gui=\"../scratch-gui\"",
    "build:legoble": "npx xcratch-build --module=\"legoble\" --block=\"scratch-vm/src/extensions/scratch3_legoble\" --entry=\"scratch-gui/src/lib/libraries/extensions/legoble\" --gui=\"../scratch-gui\"",
    "build:spikeessential": "npx xcratch-build --module=\"spikeessential\" --block=\"scratch-vm/src/extensions/scratch3_spikeessential\" --entry=\"scratch-gui/src/lib/libraries/extensions/spikeessential\" --gui=\"../scratch-gui\"",
    "build:legoremote": "npx xcratch-build --module=\"legoremote\" --block=\"scratch-vm/src/extensions/scratch3_legoremote\" --entry=\"scratch-gui/src/lib/libraries/extensions/legoremote\" --gui=\"../scratch-gui\"",
    "build:controlplus": "npx xcratch-build --module=\"controlplus\" --block=\"scratch-vm/src/extensions/scratch3_controlplus\" --entry=\"scratch-gui/src/lib/libraries/extensions/controlplus\" --gui=\"../scratch-gui\"",
    "build:poweredup": "npx xcratch-build --module=\"poweredup\" --block=\"scratch-vm/src/extensions/scratch3_poweredup\" --entry=\"scratch-gui/src/lib/libraries/extensions/poweredup\" --gui=\"../scratch-gui\"",
    "build:duplotrain": "npx xcratch-build --module=\"duplotrain\" --block=\"scratch-vm/src/extensions/scratch3_duplotrain\" --entry=\"scratch-gui/src/lib/libraries/extensions/duplotrain\" --gui=\"../scratch-gui\"",
    "build:legomario": "npx xcratch-build --module=\"legomario\" --block=\"scratch-vm/src/extensions/scratch3_legomario\" --entry=\"scratch-gui/src/lib/libraries/extensions/legomario\" --gui=\"../scratch-gui\"",
    "build:legoluigi": "npx xcratch-build --module=\"legoluigi\" --block=\"scratch-vm/src/extensions/scratch3_legoluigi\" --entry=\"scratch-gui/src/lib/libraries/extensions/legoluigi\" --gui=\"../scratch-gui\"",
    "build:legopeach": "npx xcratch-build --module=\"legopeach\" --block=\"scratch-vm/src/extensions/scratch3_legopeach\" --entry=\"scratch-gui/src/lib/libraries/extensions/legopeach\" --gui=\"../scratch-gui\"",
    "start": "cd ../scratch-gui && npm run start"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bricklife/scratch-lego-bluetooth-extensions.git"
  },
  "keywords": [
    "scratch",
    "lego",
    "bluetooth",
    "ble"
  ],
  "author": "Shinichiro Oba",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bricklife/scratch-lego-bluetooth-extensions/issues"
  },
  "homepage": "https://github.com/bricklife/scratch-lego-bluetooth-extensions",
  "devDependencies": {
    "xcratch-build": "github:bricklife/xcratch-build#lego-bluetooth-extensions"
  }
}
